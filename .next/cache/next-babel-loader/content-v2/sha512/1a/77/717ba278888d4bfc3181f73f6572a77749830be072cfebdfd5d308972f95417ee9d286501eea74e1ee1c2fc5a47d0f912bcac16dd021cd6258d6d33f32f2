       try {\n          const accounts = await window.ethereum.request({ method: \"eth_accounts\" });\n          if (accounts.length > 0) {\n            setAccount(accounts[0]);\n          }\n        } catch (err) {\n          console.error(\"Wallet check failed:\", err);\n        }\n      }\n    };\n    checkWallet();\n  }, []);\n\n  const connectWallet = async () => {\n    if (typeof window.ethereum !== \"undefined\") {\n      try {\n        const accounts = await window.ethereum.request({ method: \"eth_requestAccounts\" });\n        setAccount(accounts[0]);\n      } catch (err) {\n        console.error(\"User denied connection:\", err);\n      }\n    } else {\n      alert(\"Please install MetaMask to use this feature.\");\n    }\n  };\n\n  async function onSubmit(data) {\n    try {\n      const accounts = await web3.eth.getAccounts();\n      await factory.methods\n        .createCampaign(\n          web3.utils.toWei(data.minimumContribution, \"ether\"),\n          data.campaignName,\n          data.description,\n          data.imageUrl,\n          web3.utils.toWei(data.target, \"ether\")\n        )\n        .send({ from: accounts[0] });\n\n      router.push(\"/\");\n    } catch (err) {\n      setError(err.message);\n      console.error(\"Create campaign error:\", err);\n    }\n  }\n\n  return (\n    <div>\n      <Head>\n        <title>New Campaign</title>\n        <meta name=\"description\" content=\"Create New Campaign\" />\n        <link rel=\"icon\" href=\"../logo.png\" />\n      </Head>\n      <main>\n        <Stack spacing={8} mx={\"auto\"} maxW={\"2xl\"} py={12} px={6}>\n          <Text fontSize={\"lg\"} color={\"teal.400\"}>\n            <ArrowBackIcon mr={2} />\n            <NextLink href=\"/\"> Back to Home</NextLink>\n          </Text>\n          <Stack>\n            <Heading fontSize={\"4xl\"}>Create a New Campaign ðŸ“¢</Heading>\n          </Stack>\n          <Box\n            rounded={\"lg\"}\n            bg={useColorModeValue(\"white\", \"gray.700\")}\n            boxShadow={\"lg\"}\n            p={8}\n          >\n            <form onSubmit={handleSubmit(onSubmit)}>\n              <Stack spacing={4}>\n                <FormControl id=\"minimumContribution\">\n                  <FormLabel>Minimum Contribution Amount</FormLabel>\n                  <InputGroup>\n                    <Input\n                      type=\"number\"\n                      step=\"any\"\n                      {...register(\"minimumContribution\", { required: true })}\n                      isDisabled={isSubmitting}\n                      onChange={(e) =>\n                        setMinContriInINR(Math.abs(e.target.value))\n                      }\n                    />\n                    <InputRightAddon children=\"ETH\" />\n                  </InputGroup>\n                  {minContriInINR && (\n                    <FormHelperText>\n                      â‚¹ {getETHPriceInINR(ETHPrice, minContriInINR)}\n                    </FormHelperText>\n                  )}\n                </FormControl>\n\n                <FormControl id=\"campaignName\">\n                  <FormLabel>Campaign Name</FormLabel>\n                  <Input\n                    {...register(\"campaignName\", { required: true })}\n                    isDisabled={isSubmitting}\n                  />\n                </FormControl>\n\n                <FormControl id=\"description\">\n                  <FormLabel>Campaign Description</FormLabel>\n                  <Textarea\n                    {...register(\"description\", { required: true })}\n                    isDisabled={isSubmitting}\n                  />\n                </FormControl>\n\n                <FormControl id=\"imageUrl\">\n                  <FormLabel>Image URL</FormLabel>\n                  <Input\n                    {...register(\"imageUrl\", { required: true })}\n                    isDisabled={isSubmitting}\n                    type=\"url\"\n                  />\n                </FormControl>\n\n                <FormControl id=\"target\">\n                  <FormLabel>Target Amount</FormLabel>\n                  <InputGroup>\n                    <Input\n                      type=\"number\"\n                      step=\"any\"\n                      {...register(\"target\", { required: true })}\n                      isDisabled={isSubmitting}\n                      onChange={(e) => setTargetInINR(Math.abs(e.target.value))}\n                    />\n                    <InputRightAddon children=\"ETH\" />\n                  </InputGroup>\n                  {targetInINR && (\n                    <FormHelperText>\n                      â‚¹ {getETHPriceInINR(ETHPrice, targetInINR)}\n                    </FormHelperText>\n                  )}\n                </FormControl>\n\n                {error && (\n                  <Alert status=\"error\">\n                    <AlertIcon />\n                    <AlertDescription>{error}</AlertDescription>\n                  </Alert>\n                )}\n\n                {(errors.minimumContribution ||\n                  errors.name ||\n                  errors.description ||\n                  errors.imageUrl ||\n                  errors.target) && (\n                  <Alert status=\"error\">\n                    <AlertIcon />\n                    <AlertDescription>\n                      All Fields are Required\n                    </AlertDescription>\n                  </Alert>\n                )}\n\n                <Stack spacing={10}>\n                  {account ? (\n                    <Button\n                      bg={\"teal.400\"}\n                      color={\"white\"}\n                      _hover={{ bg: \"teal.500\" }}\n                      isLoading={isSubmitting}\n                      type=\"submit\"\n                    >\n                      Create\n                    </Button>\n                  ) : (\n                    <Stack spacing={3}>\n                      <Button\n                        color={\"white\"}\n                        bg={\"teal.400\"}\n                        _hover={{ bg: \"teal.300\" }}\n                        onClick={connectWallet}\n                      >\n                        Connect Wallet\n                      </Button>\n                      <Alert status=\"warning\">\n                        <AlertIcon />\n                        <AlertDescription>\n                          Please Connect Your Wallet First to Create a Campaign\n                        </AlertDescription>\n                      </Alert>\n                    </Stack>\n                  )}\n                </Stack>\n              </Stack>\n            </form>\n          </Box>\n        </Stack>\n      </main>\n    </div>\n  );\n}\n"]},"metadata":{},"sourceType":"module"}